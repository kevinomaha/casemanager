{
  "name": "workflow-manager-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "aws-amplify": "^4.3.43",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-oidc-context": "^2.3.1",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\" --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.23.3",
    "@babel/plugin-transform-numeric-separator": "^7.23.3",
    "@babel/plugin-transform-optional-chaining": "^7.23.3",
    "@babel/plugin-transform-private-methods": "^7.23.3",
    "@babel/plugin-transform-private-property-in-object": "^7.23.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.32.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios)/"
    ],
    "moduleNameMapper": {
      "axios": "<rootDir>/src/__mocks__/axios.js"
    }
  },
  "overrides": {
    "glob": "^10.3.10",
    "rimraf": "^5.0.5",
    "svgo": "^3.2.0",
    "workbox-background-sync": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-google-analytics": "^7.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.4.15",
    "inflight": "^2.0.0"
  },
  "resolutions": {
    "**/inflight": "^2.0.0",
    "**/glob": "^10.3.10",
    "**/rimraf": "^5.0.5",
    "**/svgo": "^3.2.0",
    "**/stable": "^0.2.0",
    "**/domexception": "^4.0.0",
    "**/q": "npm:p-queue@^7.3.4",
    "**/abab": "^2.0.6",
    "rollup-plugin-terser": "@rollup/plugin-terser@^0.4.4",
    "sourcemap-codec": "@jridgewell/sourcemap-codec@^1.4.15"
  }
}
